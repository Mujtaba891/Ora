{
    "name": "Ora File App",
    "short_name": "Ora",
    "description": "Open and edit .ora files",
    "start_url": "/index.html",
    "display": "standalone",
    "icons": [
      {
        "src": ".ora.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ],
    "file_handlers": [
      {
        "action": "/viewer.html",
        "accept": {
          "application/ora": [".ora"]
        }
      }
    ]
  }
  